table Finanzas
	lineageTag: 2d8642c6-81c5-4dfe-af6e-d522343d284b

	measure 'Total Finanzas' = SUM( Finanzas[Cantidad] )
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
		lineageTag: 6552796a-2432-4950-a951-d6a566cce053

	measure Ingresos = CALCULATE( [Total Finanzas] ,  Finanzas[Tipo] = "Ingresos")
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
		lineageTag: fda23011-3a5f-473d-9c27-e09aceebb574

	measure Gastos = CALCULATE([Total Finanzas] , Finanzas[Tipo] = "Gastos")
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
		lineageTag: 47f2aa99-ca7b-4de3-ac3c-104f49887dc9

	measure Utilidad = [Ingresos] - [Gastos]
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
		lineageTag: 3836484a-c36a-4ebd-9b9b-46bd74d232fc

	measure Saldo = CALCULATE( [Utilidad], FILTER( ALL(Finanzas) , Finanzas[Fecha] <= MAX(Finanzas[Fecha])))
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
		lineageTag: 1caaf515-632b-477a-a08b-4e759d09422f

	measure 'Sa. Esperado' = CALCULATE( [Ut. Esperada], FILTER( ALL(Expectativas), Expectativas[Fecha] <= MAX(Expectativas[Fecha])))
		formatString: \$#,0.###############;(\$#,0.###############);\$#,0.###############
